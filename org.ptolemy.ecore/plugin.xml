<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="org.ptolemy.ecore.kernel"
            class="org.ptolemy.ecore.kernel.KernelPackage"
            genModel="model/kernel.genmodel"/>
      <package
            uri="org.ptolemy.ecore.actor"
            class="org.ptolemy.ecore.actor.ActorPackage"
            genModel="model/actor.genmodel"/>
      <package
            uri="org.ptolemy.ecore.caltrop"
            class="org.ptolemy.ecore.caltrop.CaltropPackage"
            genModel="model/caltrop.genmodel"/>
      <package
            uri="org.ptolemy.ecore.xactor"
            class="org.ptolemy.ecore.xactor.XactorPackage"
            genModel="model/xactor.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="eactor"
            class="org.ptolemy.ecore.xactor.util.XactorResourceFactoryImpl"/>
   </extension>

</plugin>
